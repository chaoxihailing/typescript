{
    "extends": "tslint:recommended",
    "rules": {
        "no-console": [
            false
        ],
        "no-empty": [
            false
        ],
        "no-empty-interface":[
            false
        ],
        "object-literal-sort-keys": false,
        "object-literal-shorthand": false,
        "ordered-imports": [false],
        "jsdoc-format": false,
        "max-line-length": [true, 200],
        "prefer-const": false,
        "interface-name" : [false, "never"]
    }
}
